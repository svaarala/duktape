#
#  Example Makefile for building the eval example
#

CC = gcc
LD = $(CC)

RM = rm -f

CFLAGS = -std=c99 -Wall -Wextra -O2 -Isrc
LDFLAGS = -lm

%.o: %.c
	$(CC) $(CFLAGS) -c $< -o $@

OBJECTS = src/duktape.o examples/eval/eval.o

eval: $(OBJECTS)
	$(LD) -o $@ $(OBJECTS) $(LDFLAGS)

all: eval

clean:
	$(RM) $(OBJECTS) eval
